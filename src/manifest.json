{
  "manifest_version": 2,

  "name": "Call it a day",
  "description": "This extension tells user that it is time to call it a day",
  "version": "1.0",

  "background": {
    "scripts": ["background.js"]
  },

  "browser_action": {
    "default_popup": "popup.html"
  },

  "permissions": [
    "tabs",
    "alarms"
  ]

}
